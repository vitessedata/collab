;; group by linenumber
((groupby
	(bucket (var l_linenumber))
	(function (count (int 1)) (sum (LIT.i32 5)) (sum (* (var l_partkey) (var l_partkey)))) )
 (symtab (l_orderkey i64v) 
   (l_partkey i64v) 
   (l_suppkey i64v) 
   (l_linenumber i64v) 
   (l_quantity fp64v) 
   (l_extendedprice fp64v) 
   (l_discount fp64v) 
   (l_tax fp64v) 
   (l_returnflag strv) 
   (l_linestatus strv) 
   (l_shipdate datev) 
   (l_commitdate datev) 
   (l_receiptdate datev) 
   (l_shipinstruct strv) 
   (l_shipmode strv) 
   (l_comment strv) )
 (xrgpath lineitem.xrg)
)
